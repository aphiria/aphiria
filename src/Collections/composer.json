{
    "name": "aphiria/collections",
    "description": "The Aphiria collections library",
    "keywords": [
        "PHP",
        "collections",
        "stacks",
        "queues",
        "sets",
        "hash tables",
        "dictionaries"
    ],
    "authors": [
        {
            "name": "David Young",
            "email": "dave@aphiria.com",
            "role": "Architect"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aphiria\\Collections\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aphiria\\Collections\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "php-coveralls/php-coveralls": "^2.2",
        "phpunit/phpunit": "~9.4"
    },
    "scripts": {
        "lint-check": "php-cs-fixer fix --config=.php_cs.dist -v --diff --dry-run",
        "lint-fix": "php-cs-fixer fix --config=.php_cs.dist -v --diff"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
